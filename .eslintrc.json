{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "plugin:react/recommended",
  "overrides": [
  ],
  "parserOptions": {
    "ecmaVersion": 2020
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "react/no-unescaped-entities": 0,
    "react/prop-types": 0,
    "react/react-in-jsx-scope" : 0
  }
}
